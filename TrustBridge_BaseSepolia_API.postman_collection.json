                "exec": [
                  "if (pm.response.code === 200) {",
                  "    const jsonData = pm.response.json();",
                  "    pm.collectionVariables.set('access_token', jsonData.tokens.accessToken);",
                  "    pm.collectionVariables.set('refresh_token', jsonData.tokens.refreshToken);",
